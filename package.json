{
  "name": "meocli",
  "description": "Node CLI generated with oclif, Integrate Prettier",
  "version": "0.1.5",
  "author": "meme2046",
  "bin": {
    "me": "./bin/run.js"
  },
  "bugs": "https://github.com/meme2046/meocli/issues",
  "dependencies": {
    "@oclif/core": "^4.8.0",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "@prettier/plugin-xml": "^3.4.2",
    "d3-format": "^3.1.2",
    "dayjs": "^1.11.19",
    "debug": "^4.4.3",
    "dotenv": "^17.2.3",
    "execa": "^9.6.1",
    "js-yaml": "^4.1.1",
    "lodash": "^4.17.23",
    "prettier": "^3.8.1",
    "prettier-plugin-nginx": "^1.0.3",
    "prettier-plugin-sh": "^0.18.0",
    "prettier-plugin-toml": "^2.0.6",
    "trading-signals": "^7.4.3"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/d3-format": "^3.0.4",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.23",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "mocha": "^10",
    "npm-check-updates": "^19.3.2",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/meme2046/meocli",
  "keywords": [
    "oclif",
    "prettier",
    "formatter"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "me",
    "dirname": "me",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": "meme2046/meocli",
  "scripts": {
    "ncu": "ncu --target minor --reject webpack,shx",
    "tsv": "tsc -v",
    "dev": "node --no-deprecation --no-warnings --disable-warning=ExperimentalWarning --loader ts-node/esm bin/dev.js",
    "prod": "node bin/run.js",
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "pnpm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
